{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI Debug (with reload)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app",
        "--reload",
        "--port", "8000"
      ],
      "jinja": true,
      "console": "integratedTerminal",
      "subProcess": true
    }
  ]
}
